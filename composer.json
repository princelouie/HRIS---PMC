{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4||^8.0",
    "barryvdh/laravel-dompdf": "^2.0",
    "beyondcode/laravel-query-detector": "^1.6",
    "brian2694/laravel-toastr": "^5.56",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^2.0",
    "google/cloud": "^0.167.0",
    "google/cloud-firestore": "^1.20",
    "grpc/grpc": "^1.38",
    "guzzlehttp/guzzle": "^7.0.1",
    "intervention/image": "^2.6",
    "jenssegers/agent": "^2.6",
    "kreait/laravel-firebase": "^3.1",
    "laravel/cashier": "^13.8",
    "laravel/framework": "^8.40",
    "laravel/tinker": "^2.5",
    "laravel/ui": "^3.3",
    "laravelcollective/html": "^6.2",
    "league/flysystem-aws-s3-v3": "^1.0",
    "nesbot/carbon": "^2.53",
    "nwidart/laravel-modules": "8.1.0",
    "sentry/sentry-laravel": "^2.11",
    "simplesoftwareio/simple-qrcode": "^4.2",
    "spatie/laravel-activitylog": "^3.17",
    "spatie/laravel-analytics": "3.11",
    "symfony/mailer": "^6.1",
    "twilio/sdk": "^6.36",
    "tymon/jwt-auth": "^1.0",
    "yajra/laravel-datatables-oracle": "^9.18"
  },
  "require-dev": {
      "barryvdh/laravel-debugbar": "^3.6",
      "facade/ignition": "^2.5",
      "fakerphp/faker": "^1.9.1",
      "jason-guru/laravel-make-repository": "^0.0.3",
      "laravel/sail": "^1.0.1",
      "mockery/mockery": "^1.4.2",
      "nunomaduro/collision": "^5.0",
      "phpunit/phpunit": "^9.3.3"
      
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/",
      "Modules\\": "Modules/"
    },
    "files": [
      "app/Helpers/Helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
